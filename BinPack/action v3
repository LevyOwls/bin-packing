package bp;

public class Action {

	int mochilaPos;
	int objPos;
	
	
	public int getMochilaPos() {
		return mochilaPos;
	}


	public void setMochilaPos(int mochilaPos) {
		this.mochilaPos = mochilaPos;
	}


	public int getObjPos() {
		return objPos;
	}


	public void setObjPos(int objPos) {
		this.objPos = objPos;
	}


	public Action(int mochilaPos, int objPos) {
		this.mochilaPos = mochilaPos;
		this.objPos = objPos;
	}
	
	
	
	
	

}
