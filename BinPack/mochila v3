package bp;

import java.util.ArrayList;



public class Mochila {

	
	double pesoMax;
	double currentP;
	ArrayList <Objeto> items;
	
	
	public Mochila(double pesoMax, double currentP, ArrayList<Objeto> items) {
		this.pesoMax = pesoMax;
		this.currentP = currentP;
		this.items = items;
	}
	
	
	
	

}
